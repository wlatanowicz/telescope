{
  "compilerOptions" : {
    "module": "commonjs",
    "removeComments": true,
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@framework/*": [
        "./framework/*"
      ],
      "@app/*": [
        "./app/*"
      ]
    }
  },
  "include" : [
    "./app/**/*",
    "./framework/**/*"
  ]
}